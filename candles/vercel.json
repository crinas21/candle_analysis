{
    "version": 2,
    "builds": [
      {
        "src": "candles/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.0" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "candles/wsgi.py"
      }
    ]
  }
  